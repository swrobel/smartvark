<h1>Register</h1>

<% form_for @user, :url => account_path do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit "Register" %>
  <input type="hidden" name="authentication_type" value="user"/>
  <input type="radio" id="facebook_oauth_provider" name="oauth_provider" value="facebook"/>
<% end %>
