$("#undo_last_action").html("<%= escape_javascript(undo_last_action_link(@offer, @liked).html_safe) %>")
<% if @liked %>
  $("#my_list").prepend($("<%= escape_javascript(list_offer(@offer).html_safe) %>").fadeIn("slow"));
<% end %>
<% if @prompt_signup %>
  $("#signup-popup").modal();
<% end %>
$("#offer_<%= @offer.id %>").hide("shrink");
